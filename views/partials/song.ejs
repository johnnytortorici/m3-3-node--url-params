<ul>
    <% top50.forEach( (song) => { %>
        <li class="song-row">
            <div class="rank-info">
                <%- `<p class="rank">#${song.rank}</p>` %>
                <%- `<p class="details">(${song.streams} streams)</p>` %>
            </div>
            <div class="song-info">
                <%- `<a href="/top50/song/${song.rank}" target="_self"><p class="title">${song.title}</p></a>` %>
                <%- `<p class="artist">by ${song.artist}</p>` %>
            </div>
            <div class="publication-info">
                <%- `<p class="details">publication date: ${song.publicationDate}</p>`  %>
            </div>
        </li>
    <% }); %>
</ul>